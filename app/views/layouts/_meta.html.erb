<%- title = content_for?(:title) ? yield(:title) : 'gellaho' %>
<%- description = content_for?(:description) ? yield(:description) : 'You chose to be here, you have no one to blame but yourself.' %>

<%= csrf_meta_tags %>
<%= csp_meta_tag %>
<meta charset="utf-8" />
<meta http-equiv="x-ua-compatible" content="ie=edge">
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<%# favicon_link_tag vite_asset_url('images/favicons/favicon-32x32.png') %>
<%= content_tag :meta, nil, property: 'og:title', content: title %>
<%= content_tag :meta, nil, property: 'twitter:title', content: title %>
<%= content_tag :meta, nil, name: 'title', content: title %>
<%= content_tag :meta, nil, property: 'og:description', content: description %>
<%= content_tag :meta, nil, property: 'twitter:description', content: description %>
<%= content_tag :meta, nil, name: 'description', content: description %>
<% if content_for?(:image) %>
  <%= content_tag :meta, nil, property: 'og:image', content: yield(:image) %>
  <%= content_tag :meta, nil, property: 'og:image:secure', content: yield(:image) %>
  <%= content_tag :meta, nil, property: 'twitter:image', content: yield(:image) %>
  <%= content_tag :meta, nil, property: 'twitter:image:secure', content: yield(:image) %>
<% else %>
  <%= content_tag :meta, nil, property: 'og:image', content: vite_asset_url('images/favicons/profile.png') %>
  <%= content_tag :meta, nil, property: 'og:image:secure', content: vite_asset_url('images/favicons/profile.png') %>
  <%= content_tag :meta, nil, property: 'og:image:height', content: '5928' %>
  <%= content_tag :meta, nil, property: 'og:image:width', content: '3159' %>
<% end %>
